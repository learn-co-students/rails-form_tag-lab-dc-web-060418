<h1>Student Form</h1>
<%= form_tag students_path do %>


  <label>First Name:</label><br>
  <%= text_field_tag :'student[first_name]', params[:'student[first_name]'], id:'first_name'%><br>


  <label>Last Name:</label><br>
    <%= text_field_tag :'student[last_name]', params[:'student[last_name]'], id:'last_name'%><br>

  <input type="submit" value="Submit Student">

<% end %>
